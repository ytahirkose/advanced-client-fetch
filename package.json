{
  "name": "@advanced-client-fetch/core",
  "version": "1.1.3",
  "description": "ðŸš€ Modern HTTP client with fetch-first design, plugin architecture, and security features. Works across all platforms with automatic cookie management and SSRF protection.",
  "type": "module",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./all": {
      "types": "./dist/all.d.ts",
      "import": "./dist/all.js",
      "require": "./dist/all.cjs"
    },
    "./plugins": {
      "types": "./packages/plugins/dist/index.d.ts",
      "import": "./packages/plugins/dist/index.js",
      "require": "./packages/plugins/dist/index.cjs"
    },
    "./presets": {
      "types": "./packages/presets/dist/index.d.ts",
      "import": "./packages/presets/dist/index.js",
      "require": "./packages/presets/dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "packages/*/dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @advanced-client-fetch/core build",
    "build:plugins": "pnpm --filter @advanced-client-fetch/plugins build",
    "build:presets": "pnpm --filter @advanced-client-fetch/presets build",
    "build:axios-adapter": "pnpm --filter @advanced-client-fetch/axios-adapter build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "test:integration": "pnpm -r test:integration",
    "test:coverage": "pnpm -r test:coverage",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "clean": "pnpm -r clean",
    "typecheck": "pnpm -r typecheck",
    "analyze": "node scripts/analyze-bundle.js",
    "analyze:size": "pnpm build && pnpm analyze",
    "prepublishOnly": "pnpm build",
    "release": "pnpm build && pnpm -r publish --access public",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release-packages": "changeset publish"
  },
  "keywords": [
    "http",
    "client",
    "fetch",
    "axios",
    "request",
    "api",
    "rest",
    "graphql",
    "retry",
    "cache",
    "timeout",
    "rate-limit",
    "circuit-breaker",
    "dedupe",
    "metrics",
    "middleware",
    "plugin",
    "typescript",
    "node",
    "browser",
    "edge",
    "deno",
    "bun",
    "cloudflare",
    "vercel",
    "serverless",
    "microservice",
    "api-gateway",
    "cdn",
    "websocket",
    "streaming",
    "real-time",
    "batch",
    "production",
    "development",
    "testing"
  ],
  "author": {
    "name": "Yasar Tahir Kose",
    "email": "ytahirkose@gmail.com",
    "url": "https://github.com/ytahirkose"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ytahirkose/advanced-client-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/ytahirkose/advanced-client-fetch/issues"
  },
  "homepage": "https://github.com/ytahirkose/advanced-client-fetch#readme",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "terser": "^5.44.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
