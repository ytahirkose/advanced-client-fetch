{
  "name": "hyperhttp-axios-adapter",
  "version": "1.0.0",
  "description": "ðŸ”„ Axios compatibility layer for HyperHTTP. Drop-in replacement for Axios with HyperHTTP power. Solves CORS+cookie issues and works in Edge runtimes.",
  "type": "module",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:prod": "cross-env NODE_ENV=production tsup && tsc --emitDeclarationOnly --outDir dist",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "hyperhttp-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "http",
    "client",
    "axios",
    "compatibility",
    "adapter",
    "fetch",
    "request",
    "no-dependencies",
    "migration",
    "drop-in-replacement",
    "cors",
    "cookies",
    "edge-runtime",
    "typescript",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ytahirkose/advanced-client-fetch.git",
    "directory": "packages/axios-adapter"
  },
  "bugs": {
    "url": "https://github.com/ytahirkose/advanced-client-fetch/issues"
  },
  "homepage": "https://github.com/ytahirkose/advanced-client-fetch#readme",
  "author": "HyperHTTP Team",
  "license": "MIT"
}
